

 pipeline {
    agent any

    stages {
        stage('Checkout Code') {
            steps {
                 echo " getting project from git "
                git url: 'https://github.com/lawi55/kaddem', branch: 'imen'
            }
        }

        stage('Build Project') {
            steps {

                sh 'mvn clean'
                sh 'mvn compile'
            }
        }

         stage('MVN SonarQube') {
             environment {
                 SONAR_TOKEN = credentials('SonarQube_Cred')
             }
             steps {
                 sh '''
                 mvn sonar:sonar \
                 -Dsonar.host.url=http://192.168.50.4:9000 \
                 -Dsonar.login=$SONAR_TOKEN
                 '''
             }
         }


    }
}
